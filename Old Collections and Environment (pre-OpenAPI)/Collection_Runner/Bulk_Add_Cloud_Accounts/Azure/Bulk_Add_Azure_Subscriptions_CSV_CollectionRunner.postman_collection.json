	"item": [
		{
			"name": "Login Generate Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"postman.setEnvironmentVariable(\"token\", jsonData.token);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "accept",
						"value": "application/json; charset=UTF-8"
					},
					{
						"key": "content-type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"username\": \"ACCESS KEY\",\n    \"password\": \"SECRET KEY\"\n}"
				},
				"url": {
					"raw": "https://{{api-endpoint}}/login",
					"protocol": "https",
					"host": [
						"{{api-endpoint}}"
					],
					"path": [
						"login"
					]
				},
				"description": "Returns a JWT auth token for accessing the Prisma Cloud APIs. To generate a token, you must have an access key and include the following values in the request body parameter â€” access key ID as the username and your secret key as the password. Prisma Cloud requires this JWT in the request header to authorize API access."
			},
			"response": []
		},
		{
